{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/styles.css",
      "use": "@vercel/static"
    },
    {
      "src": "public/script.js",
      "use": "@vercel/static"
    },
    {
      "src": "public/list.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/app.js"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/list",
      "dest": "/public/list.html"
    },
    {
      "src": "/styles.css",
      "dest": "/public/styles.css"
    },
    {
      "src": "/script.js",
      "dest": "/public/script.js"
    }
  ]
}
