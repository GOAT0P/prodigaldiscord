{
  "version": 2,
  "functions": {
    "api/index.js": { "runtime": "nodejs22.x" }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/list",
      "dest": "/public/list.html"
    }
  ]
}
