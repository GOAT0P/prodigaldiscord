{
  "version": 2,
  "functions": {
    "api/index.js": { "runtime": "nodejs22.x" }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/discord-bot/(.*)",
      "dest": "/discord-bot/bot.js"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/list",
      "dest": "/public/list.html"
    },
    {
      "src": "/styles.css",
      "dest": "/public/styles.css"
    },
    {
      "src": "/script.js",
      "dest": "/public/script.js"
    }
  ],
  "staticFiles": {
    "public": "public"
  }
}
